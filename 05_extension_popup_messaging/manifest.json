{
    "manifest_version": 2,
    "name": "Extension Pop Up Messaging",
    "version": "0.1",
    "permissions": ["tabs"],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "sketch/index.html",
        "default_title": "Paragragh Replacer"
    }
}